{
    "manifest_version": 2,
    "name": "close other tabs",
    "version": "1.0",
    "default_locale": "en",
    "description": "close other tabs",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon48.png",
        "default_title": "close other tabs"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
}