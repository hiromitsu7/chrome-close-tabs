{
    "manifest_version": 2,
    "name": "close other tabs",
    "version": "1.0",
    "description": "close other tabs",
    "icons": {
        "48": "icon48.png"
    },
    "browser_action": {
        "default_icon": "icon48.png",
        "default_title": "close other tabs"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "permissions": ["tabs"]
}